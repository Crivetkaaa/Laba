        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laba"
        mc:Ignorable="d"
        Title="Window_working" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="userlogin" HorizontalAlignment="Left" Margin="400,194,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <Label Content="Фамилия сотрудника" HorizontalAlignment="Left" Margin="240,190,0,0" VerticalAlignment="Top"/>
        <Button x:Name="del_button" Content="Удалить" HorizontalAlignment="Center" Margin="0,302,0,0" VerticalAlignment="Top" Height="37" Width="122" Click="del_button_Click"/>

    </Grid>
</Window>
